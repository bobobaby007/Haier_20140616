package  {	import flash.display.MovieClip;	import gs.TweenLite		import flash.geom.Point;	public class MainBg extends MovieClip{		static public var _self:MainBg		private var _pNum:Number=1		private var _picNum:int=1		private var _p:Point		private var _Xms:Array=[220,150,166,204]		public function MainBg() {			_self=this			_PicNum=1		}		public function set _PicNum(__set:int){			_picNum=__set			_bgs.gotoAndStop(__set)			_bgs.alpha=0			_bgs._product.visible=false			TweenLite.to(_bgs,2,{alpha:1})						_bgs.y=-536+_Xms[_picNum-1]			trace(_bgs.y)			if(ItemsBoard._self){				ItemsBoard._self._clearItem()			}			switch(_picNum){				case 1:									break			}		}				public function get _Movied():Number{			return _Xms[_picNum-1]		}		public function _forSave(){			AlertProduct._self._hide()		}		public function set _ProductNum(__set:int){			_pNum=__set			_bgs._product.alpha=1			_bgs._product.gotoAndStop(_pNum)			_bgs._product.visible=true			var _point:Point=new Point(_bgs._product.x,_bgs._product.y)			_point=_bgs.localToGlobal(_point)			AlertProduct._self.x=_point.x			AlertProduct._self.y=_point.y						AlertProduct._self._show()		}		public function _reset(){			_PicNum=_pNum		}	}	}