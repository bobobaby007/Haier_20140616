package {	import flash.net.navigateToURL	import flash.net.URLRequest	import flash.external.ExternalInterface	import flash.net.URLLoader;	import flash.events.Event;	public class Navigate {		public function Navigate() {		}		static public function gotoURL(_url:String,_str:String="_blank") {			try{				var browser=ExternalInterface.call("function getBrowser(){return navigator.userAgent;}") as String;				if(browser){					browser=browser.toLowerCase()					//throw(browser)				}							if (browser && browser.indexOf("msie") != -1|| browser.indexOf("chrome") != -1) {//(browser.indexOf("firefox") != -1 ||					ExternalInterface.call( "window.open", _url, _str );					return				} else {					navigateToURL(new URLRequest(_url),_str);					return				}			}catch(error){				navigateToURL(new URLRequest(_url),_str);			}		}				/*function fl_ClickToGoToWebPage_2(event:MouseEvent):void {			var temp_str2:String="http://p.178ugame.com/s/1/1552/18331.html?uid=113201";			if (Capabilities.playerType == "ActiveX") {				ExternalInterface.call("window.open",String(temp_str2),"_blank");			} else {				navigateToURL(new URLRequest(temp_str2),"_blank");			}		}*/	}}