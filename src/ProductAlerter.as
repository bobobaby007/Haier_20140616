package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import gs.TweenLite	import gs.easing.Back	import gs.easing.Linear	public class ProductAlerter extends MovieClip{		public var _canBreath:Boolean=false		public var _breathScale:Number=1.1		static public var _self:ProductAlerter		public function ProductAlerter() {				_self=this			this.mouseEnabled=false			this.mouseChildren=false			this.scaleX=this.scaleY=0		}		public function _breath(){			_canBreath=true			TweenLite.to(this,1,{scaleX:_breathScale,scaleY:_breathScale,ease:Linear.easeNone,onComplete:_BreathOut})		}		public function _out(){			TweenLite.to(this,.4,{scaleX:.5,scaleY:.5,alpha:0,onComplete:_hide})		}		public function set _ShowNum(__set:int){			this.gotoAndStop(__set)			_show()		}		public function _hide(){			this.visible=false		}		public function _show(){			this.visible=true			this.scaleX=this.scaleY=this.alpha=0			TweenLite.to(this,.4,{scaleX:.7,scaleY:.7,alpha:1,ease:Back.easeOut})		}		public function _BreathOut(){			TweenLite.to(this,1,{scaleX:1,scaleY:1,ease:Linear.easeNone,onComplete:_breath})		}	}	}