package  {	import flash.display.MovieClip;	import gs.TweenLite	import gs.easing.Back	public class PreloaderM extends MovieClip{		static public var _self:PreloaderM		public function PreloaderM() {			_self=this			this.visible=false			//_hide()			this.mouseEnabled=false			this.mouseChildren=false		}		public function _showAt(__set:int){			this.gotoAndStop(__set)			_show()		}		public function _show(){			this.visible=true			this.scaleX=this.scaleY=0			TweenLite.to(this,.4,{ease:Back.easeOut,scaleX:1,scaleY:1})		}		public function _hide(){			TweenLite.to(this,.4,{scaleX:0,scaleY:0,onComplete:hideOk})					}		private function hideOk(){			this.visible=false		}	}	}