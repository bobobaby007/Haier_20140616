package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import gs.TweenLite	import gs.easing.Back	import gs.easing.Linear	public class ButtonParent extends MovieClip{		public var _canBreath:Boolean=false		public var _breathScale:Number=1.1		public function ButtonParent() {				//this["_over_mc"].alpha=0			this.buttonMode=true			//_bg.alpha=.5			this.addEventListener(MouseEvent.MOUSE_OVER,mouseHander)			this.addEventListener(MouseEvent.MOUSE_OUT,mouseHander)			this.addEventListener(MouseEvent.CLICK,mouseHander)		}		private function mouseHander(event:MouseEvent){			switch(event.type){				case MouseEvent.MOUSE_OVER:					//TweenLite.to(_bg,.4,{alpha:1})					//TweenLite.to(this["_over_mc"],.4,{alpha:1})					TweenLite.to(this,.2,{scaleX:1.1,scaleY:1.1})				break				case MouseEvent.MOUSE_OUT:					//TweenLite.to(_bg,.4,{alpha:.5})					TweenLite.to(this,.4,{scaleX:1,scaleY:1})				break				case MouseEvent.CLICK:									break			}		}		public function set _Selected(__set:Boolean){			if(__set){							}else{							}		}		public function _Breath(){			_canBreath=true			TweenLite.to(this,1,{scaleX:_breathScale,scaleY:_breathScale,ease:Linear.easeNone,onComplete:_BreathOut})		}		public function _BreathOut(){			TweenLite.to(this,1,{scaleX:1,scaleY:1,ease:Linear.easeNone,onComplete:_Breath})		}	}	}