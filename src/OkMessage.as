package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import gs.TweenLite	import gs.easing.Back	public class OkMessage extends MovieClip{		static public var _self:OkMessage		public function OkMessage() {			_self=this			setup()			this.visible=false		}				private function setup(){						_again.buttonMode=true			_again.addEventListener(MouseEvent.CLICK,mouseHander)			_view.buttonMode=true			_view.addEventListener(MouseEvent.CLICK,mouseHander)		}				public function set _Name(__set:String){			_name.text=__set		}		public function set _Image(__set:String){			ImageLoader.loadAt(__set,_img,60,60)		}				public function set _AgeNum(__set:int){			_texts.gotoAndStop(__set)					}			private function mouseHander(event:MouseEvent){			switch(event.currentTarget){				case _again:					MainStage._self._GATracker.trackEvent("button","click","playAgain")					_hide()					Main._self._again()				break				case _view:					MainStage._self._GATracker.trackEvent("button","click","viewMyWork")					Navigate.gotoURL(DouBan._Self._picUrl)				break			}		}		public function _hide(){					this.visible=false					}		public function _show(){			this.visible=true			this.alpha=0			TweenLite.to(this,0.4,{alpha:1,ease:Back.easeOut})		}	}	}