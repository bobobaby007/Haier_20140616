package  {	import flash.display.MovieClip;	import gs.TweenLite	public class LoadingIntro extends MovieClip{		public function LoadingIntro() {			for(var i:int=1;i<=9;i++){				var _item:LoadingIntro_item=new LoadingIntro_item()				_item.gotoAndStop(i)				_item.y=10				_item.alpha=0				addChild(_item)				TweenLite.to(_item,8-.3*i,{alpha:1,y:-10,delay:.8*i})							}		}			}	}