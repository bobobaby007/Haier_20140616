package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import gs.TweenLite	import gs.easing.Back	import gs.easing.Linear	public class AlertParent extends MovieClip{		public var _canBreath:Boolean=false		public var _breathScale:Number=1.1		public function AlertParent() {				this.mouseEnabled=false			this.mouseChildren=false		}		public function _breath(){			_canBreath=true			TweenLite.to(this,1,{scaleX:_breathScale,scaleY:_breathScale,ease:Linear.easeNone,onComplete:_BreathOut})		}		public function _out(){			TweenLite.to(this,.4,{scaleX:0,scaleY:0,onComplete:_hide})		}		public function _hide(){			this.visible=false		}		public function _show(){			this.visible=true			this.scaleX=this.scaleY=0			TweenLite.to(this,.4,{scaleX:1,scaleY:1,ease:Back.easeOut})		}		public function _BreathOut(){			TweenLite.to(this,1,{scaleX:1,scaleY:1,ease:Linear.easeNone,onComplete:_breath})		}	}	}