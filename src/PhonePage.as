package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import gs.TweenLite	import gs.easing.Back	public class PhonePage extends MovieClip{		static public var _self:PhonePage		public function PhonePage() {			_self=this			setup()			_hide()		}		private function setup(){			_btn.addEventListener(MouseEvent.CLICK,btnHander)		}		private function btnHander(event:MouseEvent){			//Main._self._Page="drawingPage"			Main._self._Page="handOrKeyboard"					}		public function _refreshStr(__str:String){			QR_Code(_code)._refreshStr(__str,230,230)		}		public function _show(){			this.visible=true			this.scaleX=this.scaleY=0			TweenLite.to(this,.4,{ease:Back.easeOut,scaleX:1,scaleY:1})		}		public function _hide(){			this.visible=false		}	}	}